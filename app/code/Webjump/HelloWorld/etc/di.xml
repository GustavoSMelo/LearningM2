<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Framework\App\Action\Action">
        <plugin name="CustomAction" type="Webjump\HelloWorld\Plugin\CustomAction" sortOrder="99" disabled="true" />
    </type>

    <type name="Webjump\HelloWorld\Model\DebugHandler">
        <arguments>
            <argument name="filesystem" xsi:type="object">Magento\Framework\Filesystem\Driver\File</argument>
        </arguments>
    </type>
    <type name="Webjump\HelloWorld\Logger\CustomLogger">
        <arguments>
            <argument name="name" xsi:type="string">CustomLogger</argument>
            <argument name="handlers" xsi:type="array">
                <item name="system" xsi:type="object">Webjump\HelloWorld\Model\DebugHandler</item>
            </argument>
        </arguments>
    </type>
    <type name="Magento\Framework\Console\CommandListInterface">
        <arguments>
           <argument name="commands" xsi:type="array">
               <item name="my_custom_command" xsi:type="object">Webjump\HelloWorld\Console\Command\CustomCommand</item>
            </argument>
        </arguments>
    </type>
    <preference for="Webjump\HelloWorld\Api\MyApiInterface" type="Webjump\HelloWorld\Model\MyApi" />
</config>
